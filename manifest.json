{
  "manifest_version": 3,
  "name": "tlt",
  "description": "Minimum todo list and time tracking.",
  "version": "0.1.1",
  "action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "eventPage.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "alarms"
  ]
}
