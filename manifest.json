{
  "manifest_version": 3,
  "name": "TLT : ToDo List & Time Tracking",
  "description": "ToDo and budget control with a minimal UI.",
  "version": "0.4.0",
  "action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "eventPage.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "alarms"
  ]
}
