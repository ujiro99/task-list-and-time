{
  "name": "tlt",
  "version": "0.0.1",
  "description": "This is a chrome extension, enables to manage tasks and time.",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts src/; stylelint src/**/*.css",
    "format": "prettier --write src/**/*",
    "watch": "webpack -w --config webpack.dev.js",
    "zip": "find . -name \".DS_Store\" -delete; cd dist; zip -r ../build/build-$(node -p -e \"require('../package.json').version\").zip ./",
    "bump": "mversion p -m",
    "bump:i": "mversion i -m",
    "test": "jest --coverage",
    "test:w": "jest --watch --coverage"
  },
  "author": "Yujiro Takeda",
  "license": "MIT",
  "dependencies": {
    "@dnd-kit/core": "^5.0.1",
    "@dnd-kit/sortable": "^6.0.0",
    "@dnd-kit/utilities": "^3.1.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "react": "^17.0.2",
    "react-calendar": "^3.7.0",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-modal": "^3.14.4",
    "react-scripts": "4.0.3",
    "react-textarea-autosize": "^8.3.3",
    "react-transition-group": "^4.4.2",
    "recoil": "^0.7.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/chrome": "^0.0.171",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.27",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.2",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^6.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^27.3.1",
    "mini-css-extract-plugin": "^2.5.3",
    "mversion": "^2.0.1",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-nested-vars": "^1.0.0",
    "prettier": "^2.4.1",
    "style-loader": "^0.20.3",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "tailwindcss": "^3.0.12",
    "ts-jest": "^27.0.7",
    "ts-loader": "^4.0.1",
    "typescript": "^4.4.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "web-vitals": "^1.1.2",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^4.2.2",
    "webpack-notifier": "^1.15.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
